@article{Podesta2005,
abstract = {Many proteins "bind" DNA through positively charged amino acids on their surfaces. However, to overcome significant energetic and topological obstacles, proteins that bend or package DNA might also modulate the stiffness that is generated by repulsions between phosphates within DNA. Much previous work describes how ions change the flexibility of DNA in solution, but when considering macromolecules such as chromatin in which the DNA contacts the nucleosome core each turn of the double helix, it may be more appropriate to assess the flexibility of DNA on charged surfaces. Mica coated with positively charged molecules is a convenient substrate upon which the flexibility of DNA may be directly measured with a scanning force microscope. In the experiments described below, the flexibility of DNA increased as much as fivefold depending on the concentration and type of polyamine used to coat mica. Using theory that relates charge neutralization to flexibility, we predict that phosphate repulsions were attenuated by approximately 50\% in the most flexible DNA observed. This simple method is an important tool for investigating the physiochemical causes and molecular biological effects of DNA flexibility, which affects DNA biochemistry ranging from chromatin stability to viral encapsulation.},
author = {Podest\`{a}, Alessandro and Indrieri, Marco and Brogioli, Doriano and Manning, Gerald S and Milani, Paolo and Guerra, Rosalinda and Finzi, Laura and Dunlap, David},
doi = {10.1529/biophysj.105.064667},
file = {:home/ben/lori/papers/dnarna\_mechanics/podesta2005.pdf:pdf},
issn = {0006-3495},
journal = {Biophysical journal},
keywords = {Aluminum Silicates,Aluminum Silicates: chemistry,Anions,DNA,DNA: analysis,DNA: chemistry,DNA: radiation effects,DNA: ultrastructure,Elasticity,Electrochemistry,Electrochemistry: methods,Electromagnetic Fields,Nucleic Acid Conformation,Nucleic Acid Conformation: radiation effects,Static Electricity,Stress, Mechanical},
month = oct,
number = {4},
pages = {2558--63},
pmid = {16040760},
title = {{Positively charged surfaces increase the flexibility of DNA.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1366755\&tool=pmcentrez\&rendertype=abstract},
volume = {89},
