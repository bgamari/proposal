month = may,
number = {9},
pages = {2996--3009},
pmid = {17293410},
title = {{Force unfolding kinetics of RNA using optical tweezers. I. Effects of experimental variables on measured results.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1852336\&tool=pmcentrez\&rendertype=abstract},
