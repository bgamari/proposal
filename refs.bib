journal = {J. Phys. Chem.},
keywords = {Anisotropy,Carbocyanines,Carbocyanines: analysis,Carbocyanines: chemistry,DNA,Fluorescence,Fluorescent Dyes,Fluorescent Dyes: chemistry,Isomerism,Photolysis,Quantum Theory,Single-Stranded,Single-Stranded: chemistry},
month = sep,
