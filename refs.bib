keywords = {Algorithms,Computer Simulation,Dextrans,Dextrans: chemistry,Diffusion,Fluorescence Resonance Energy Transfer,Fluorescence Resonance Energy Transfer: methods,Fluorescent Dyes,Liposomes,Liposomes: chemistry,Membrane Fluidity,Models, Chemical,Models, Molecular,Motion,Phosphatidylcholines,Phosphatidylcholines: chemistry,Reproducibility of Results,Sensitivity and Specificity,Spectrometry, Fluorescence,Spectrometry, Fluorescence: methods,Streptavidin,Streptavidin: chemistry},
month = mar,
