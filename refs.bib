journal = {Science (New York, N.Y.)},
keywords = {Base Pairing,Crystallography,DNA,DNA: chemistry,Gold,Mathematics,Metal Nanoparticles,Models,Molecular,Nucleic Acid Conformation,Oligodeoxyribonucleotides,Oligodeoxyribonucleotides: chemistry,Scattering,Small Angle,Static Electricity,X-Ray,X-Ray Diffraction},
month = oct,
number = {5900},
pages = {446--9},
pmid = {18927394},
title = {{Remeasuring the double helix.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19644093},
volume = {322},
year = {2008}
}
