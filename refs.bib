journal = {Biopolymers},
keywords = {Circular Dichroism,DNA, Bacterial,DNA, Bacterial: chemistry,DNA, Superhelical,DNA, Superhelical: chemistry,Nucleic Acid Conformation,Plasmids,Plasmids: chemistry,Polyethylene Glycols,Thermodynamics},
month = feb,
number = {2},
pages = {204--17},
pmid = {11093119},
title = {{Effect of polyethylene glycol on the supercoiling free energy of DNA.}},
