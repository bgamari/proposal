file = {:home/ben/lori/papers/osmotics/rangel2006.pdf:pdf},
journal = {Biopolymers},
keywords = {betaine glycine,circular,dichroism spectroscopy,dna,ethylene glycol,fluorescence polarization anisotropy,fluorimetry,hyrdrodynamic radius,secondary structure,torsion elastic constant},
number = {3},
pages = {222--232},
title = {{Effects of ethylene glycol on the torsion elastic constant and hydrodynamic radius of p30$\backslash$delta DNA}},
volume = {85},
