@article{Parsegian2000,
abstract = {There has been much confusion recently about the relative merits of different approaches, osmotic stress, preferential interaction, and crowding, to describe the indirect effect of solutes on macromolecular conformations and reactions. To strengthen all interpretations of measurements and to forestall further unnecessary conceptual or linguistic confusion, we show here how the different perspectives all can be reconciled. Our approach is through the Gibbs-Duhem relation, the universal constraint on the number of ways it is possible to change the temperature, pressure, and chemical potentials of the several components in any thermodynamically defined system. From this general Gibbs-Duhem equation, it is possible to see the equivalence of the different perspectives and even to show the precise identity of the more specialized equations that the different approaches use.},
author = {Parsegian, V a and Rand, R P and Rau, D C},
