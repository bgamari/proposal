abstract = {We used a force-measuring laser tweezers apparatus to determine the elastic properties of lambda-bacteriophage DNA as a function of ionic strength and in the presence of multivalent cations. The electrostatic contribution to the persistence length P varied as the inverse of the ionic strength in monovalent salt, as predicted by the standard worm-like polyelectrolyte model. However, ionic strength is not always the dominant variable in determining the elastic properties of DNA. Monovalent and multivalent ions have quite different effects even when present at the same ionic strength. Multivalent ions lead to P values as low as 250-300 A, well below the high-salt "fully neutralized" value of 450-500 A characteristic of DNA in monovalent salt. The ions Mg2+ and Co(NH3)63+, in which the charge is centrally concentrated, yield lower P values than the polyamines putrescine2+ and spermidine3+, in which the charge is linearly distributed. The elastic stretch modulus, S, and P display opposite trends with ionic strength, in contradiction to predictions of macroscopic elasticity theory. DNA is well described as a worm-like chain at concentrations of trivalent cations capable of inducing condensation, if condensation is prevented by keeping the molecule stretched. A retractile force appears in the presence of multivalent cations at molecular extensions that allow intramolecular contacts, suggesting condensation in stretched DNA occurs by a "thermal ratchet" mechanism.},
author = {Baumann, C G and Smith, S B and Bloomfield, V a and Bustamante, C},
file = {:home/ben/lori/papers/dnarna\_mechanics/baumann1997.pdf:pdf},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Cobalt,DNA,DNA: chemistry,Elasticity,Entropy,Ions,Magnesium,Models, Chemical,Osmolar Concentration,Putrescine,Quaternary Ammonium Compounds,Spermidine,Static Electricity,Thermodynamics},
month = jun,
number = {12},
pages = {6185--90},
pmid = {9177192},
title = {{Ionic effects on the elasticity of single DNA molecules.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=21024\&tool=pmcentrez\&rendertype=abstract},
volume = {94},
year = {1997}
}
