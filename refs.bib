issn = {1542-0086},
journal = {Biophysical journal},
keywords = {Carbocyanines,Carbocyanines: chemistry,DNA,DNA: analysis,DNA: chemistry,DNA: genetics,Fluorescent Dyes,Fluorescent Dyes: chemistry,In Situ Hybridization, Fluorescence,In Situ Hybridization, Fluorescence: methods,Kinetics,Nucleotides,Nucleotides: analysis,Nucleotides: chemistry,Nucleotides: genetics,Sequence Analysis, DNA,Sequence Analysis, DNA: methods,Spectrometry, Fluorescence,Spectrometry, Fluorescence: methods},
month = jul,
number = {2},
pages = {729--37},
pmid = {18424494},
title = {{Tunable blinking kinetics of cy5 for precise DNA quantification and single-nucleotide difference detection.}},
