keywords = {Animals,DNA,DNA: chemistry,DNA: genetics,DNA: metabolism,Fungal Proteins,Fungal Proteins: metabolism,G-Quadruplexes,Humans,Yeasts,Yeasts: genetics,Yeasts: metabolism},
