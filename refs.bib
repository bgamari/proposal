journal = {Biophysical journal},
keywords = {Base Pairing,DNA,DNA: chemistry,Elasticity,Ethidium,Fluorescence Polarization,Kinetics,Methylene Blue,Models, Theoretical,Nucleic Acid Conformation,Osmolar Concentration},
