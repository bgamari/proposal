@article{Rambo2011,
abstract = {Unstructured proteins, RNA or DNA components provide functionally important flexibility that is key to many macromolecular assemblies throughout cell biology. As objective, quantitative experimental measures of flexibility and disorder in solution are limited, small angle scattering (SAS), and in particular small angle X-ray scattering (SAXS), provides a critical technology to assess macromolecular flexibility as well as shape and assembly. Here, we consider the Porod-Debye law as a powerful tool for detecting biopolymer flexibility in SAS experiments. We show that the Porod-Debye region fundamentally describes the nature of the scattering intensity decay by capturing the information needed for distinguishing between folded and flexible particles. Particularly for comparative SAS experiments, application of the law, as described here, can distinguish between discrete conformational changes and localized flexibility relevant to molecular recognition and interaction networks. This approach aids insightful analyses of fully and partly flexible macromolecules that is more robust and conclusive than traditional Kratky analyses. Furthermore, we demonstrate for prototypic SAXS data that the ability to calculate particle density by the Porod-Debye criteria, as shown here, provides an objective quality assurance parameter that may prove of general use for SAXS modeling and validation.},
author = {Rambo, Robert P and Tainer, John a},
doi = {10.1002/bip.21638},
file = {:home/ben/lori/papers/dnarna\_mechanics/rambo2011.pdf:pdf},
issn = {0006-3525},
journal = {Biopolymers},
keywords = {Crystallography, X-Ray,DNA,DNA: chemistry,Magnetic Resonance Spectroscopy,Models, Chemical,Nucleic Acid Conformation,Protein Folding,Proteins,Proteins: chemistry,RNA,RNA: chemistry,Scattering, Small Angle,Solutions,X-Ray Diffraction,X-Ray Diffraction: statistics \& numerical data},
month = aug,
number = {8},
pages = {559--71},
pmid = {21509745},
title = {{Characterizing flexible and intrinsically unstructured biological macromolecules by SAS using the Porod-Debye law.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3103662\&tool=pmcentrez\&rendertype=abstract},
volume = {95},
year = {2011}
}
