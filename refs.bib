keywords = {Computational Biology,Computational Biology: methods,Gene Regulatory Networks,Kinetics,Models, Statistical,Regression Analysis,Signal Transduction,Uncertainty},
month = may,
number = {10},
pages = {1300--6},
pmid = {19289448},
title = {{Gaussian process regression bootstrapping: exploring the effects of uncertainty in time course data.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2677737\&tool=pmcentrez\&rendertype=abstract},
