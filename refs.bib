issn = {1362-4962},
journal = {Nucleic acids research},
keywords = {Computational Biology,DNA,DNA: analysis,DNA: chemistry,G-Quadruplexes,Genome, Human,Genomics,Guanine,Guanine: chemistry,Humans,Nucleic Acid Conformation,Promoter Regions, Genetic,Transcription Initiation Site},
month = jan,
number = {2},
pages = {406--13},
pmid = {17169996},
title = {{G-quadruplexes in promoters throughout the human genome.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1802602\&tool=pmcentrez\&rendertype=abstract},
volume = {35},
year = {2007}
