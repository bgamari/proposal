pages = {799--804},
pmid = {22203973},
title = {{Ionic strength-dependent persistence lengths of single-stranded RNA and DNA.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3271905\&tool=pmcentrez\&rendertype=abstract},
