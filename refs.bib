issn = {0006-3525},
journal = {Biopolymers},
keywords = {Animals,Cations, Divalent,Cations, Divalent: chemistry,DNA,DNA: chemistry,Humans,Monte Carlo Method,Poisson Distribution,Static Electricity},
month = jun,
